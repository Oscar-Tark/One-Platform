  </configSections>
  <connectionStrings>
    <add name="Scorpion.Properties.Settings.SC_suggestConnectionString" connectionString="Data Source=|DataDirectory|\SC_suggest.sdf" providerName="Microsoft.SqlServerCe.Client.3.5"/>
  </connectionStrings>
  <userSettings>
    <Scorpion.Properties.Settings>
      <setting name="fst" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="lib_ver" serializeAs="String">
        <value>0.1.1</value>
      </setting>
      <setting name="eng_ver" serializeAs="String">
        <value>0.1.9b</value>
      </setting>
      <setting name="wwrap" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="fscreen" serializeAs="String">
        <value>Normal</value>
      </setting>
      <setting name="expnd" serializeAs="String">
        <value>False</value>
      </setting>
    </Scorpion.Properties.Settings>
  </userSettings>
  <startup>
